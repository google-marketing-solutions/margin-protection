{
  "name": "@launch_monitor/dv360",
  "type": "module",
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^4.9.3"
  },
  "paths": {
    "#dv360_api/*": [
      "./dv360_api/*"
    ]
  },
  "scripts": {
    "build": "tsup",
    "push": "pnpm build && clasp push",
    "push:client": "pnpm --filter client push",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "pnpm --filter ts lint dv360/src/**/*.ts"
  },
  "dependencies": {
    "@types/google-apps-script": "^2.0.7",
    "vitest": "^3.2.4"
  }
}
