{
  "name": "@launch_monitor/sa360",
  "type": "module",
  "devDependencies": {
    "@types/google-apps-script": "^1.0.56",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "gas-webpack-plugin": "^2.3.0",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "tsup": "^8.5.0",
    "typescript": "^4.9.3",
    "webpack-cli": "^5.0.1"
  },
  "scripts": {
    "build": "pnpm -w generate-version && pnpm install && cp -rf ../common/html . && webpack",
    "build:dev": "pnpm build --mode development --devtool source-map",
    "build:prod": "pnpm build --mode production",
    "push": "pnpm build:dev && clasp push",
    "push:prod": "pnpm build:prod && clasp push",
    "push:client": "pnpm --filter client push",
    "test": "vitest run",
    "lint": "pnpm --filter ts lint dv360/src/**/*.ts"
  },
  "dependencies": {
    "vitest": "^3.2.4"
  }
}
