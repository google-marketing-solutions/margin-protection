{
  "name": "@launch_monitor/sa360",
  "type": "module",
  "devDependencies": {
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "gas-webpack-plugin": "^2.3.0",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "tsup": "^8.5.0",
    "typescript": "^4.9.3",
    "webpack-cli": "^5.0.1"
  },
  "scripts": {
    "build": "tsup",
    "build:dev": "pnpm build",
    "build:prod": "pnpm build",
    "push": "pnpm build:dev && clasp push -P .clasp.development.json",
    "push:prod": "pnpm build:prod && clasp push -P .clasp.production.json",
    "push:client": "pnpm --filter client push",
    "test": "vitest run",
    "lint": "pnpm --filter ts lint dv360/src/**/*.ts"
  },
  "dependencies": {
    "@types/google-apps-script": "^2.0.7",
    "vitest": "^3.2.4"
  }
}
